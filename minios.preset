[!preset:base]
KERNEL_START_ADDR=0xc0600000

[!preset:bootfs-fat32]

[!preset:rootfs-fat32]
ROOTFS=fat32
ROOTFS_PART_INDEX=2

[!preset:rootfs-orangefs]
ROOTFS=orangefs
ROOTFS_PART_INDEX=2

[!preset:x86:inherit(base)]
ARCH=x86

[preset:i386:inherit(x86,rootfs-fat32)]
BOOT=mbr

[preset:i386]
ARCH=x86
ARCH_BITWIDTH=32
ARCH_ELF_TYPE=i386
BOOT=mbr
BOOTFS=fat32
BOOTFS_PART_INDEX=1
ROOTFS=fat32
ROOTFS_PART_INDEX=2
KERNEL_START_ADDR=0xc0600000
QEMU=qemu-system-i386
QEMU_DISPLAY=curses
QEMU_MEMORY=512

[preset:ARMv7-A:inherit(base)]
ARCH=arm
BOOT=efi
ROOTFS=ext4
ROOTFS_PART_INDEX=2
